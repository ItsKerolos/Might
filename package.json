{
  "name": "might-ui",
  "version": "0.1.0",
  "description": "A no-code solution for performing end-to-end tests",
  "scripts": {
    "prepare": "npm run build",
    "postpublish": "npm version patch --no-git",
    "svgo": "svgo --config='{ \"plugins\": [ { \"removeAttrs\": { \"attrs\": \"(fill|stroke)\" } }, { \"removeViewBox\": false } ] }' public/icons",
    "build": "webpack --config webpack.config.prod.js --mode production",
    "start": "webpack-dev-server --config webpack.config.dev.js --mode development"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ItsKerolos/might.git"
  },
  "author": "Kerolos Zaki",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ItsKerolos/might/issues"
  },
  "homepage": "https://github.com/ItsKerolos/might#readme",
  "dependencies": {
    "file-saver": "^2.0.2",
    "flcss": "^0.1.2",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-svg": "^11.0.28"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.10.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "eslint": "^7.3.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "eslint-plugin-security": "^1.4.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
